{
 "$schema": "https://cli.sasjs.io/sasjsconfig-schema.json",
 "serviceConfig": {
  "serviceFolders": [
   "sasjs/services/common",
   "sasjs/services/admin"
  ]
 },
 "macroFolders": [
  "sasjs/macros"
 ],
 "targets": [
  {
   "name": "viya",
   "serverType": "SASVIYA",
   "appLoc": "/Public/app/viyatoken",
   "buildConfig":{
     "buildOutputFileName": "myviyadeploy.sas"
   },
   "serverUrl": "http://sas.analytium.co.uk",
   "deployConfig": {
    "deployServicePack": false,
    "deployScripts": [
     "sasjs/build/copyscript.sh"
    ]
   },
   "streamConfig": {
    "assetPaths": [],
    "streamWeb": true,
    "streamWebFolder": "webv",
    "webSourcePath": "src"
   }
  }]
}
